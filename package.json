{
  "name": "green-bear",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn codegen:watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "bash setup.sh",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "prisma:push": "npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "prisma": "yarn prisma:push && yarn prisma:generate && prisma studio",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@nexus/schema": "^0.20.1",
    "@prisma/client": "^2.26.0",
    "@xstate/react": "^1.4.0",
    "apollo-server-micro": "^2.25.2",
    "formik": "^2.2.9",
    "graphql": "^15.5.1",
    "isomorphic-unfetch": "^3.1.0",
    "next": "11.0.1",
    "next-urql": "^3.1.0",
    "nexus": "^1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "urql": "^2.0.4",
    "xstate": "^4.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/near-operation-file-preset": "^1.18.4",
    "@graphql-codegen/typed-document-node": "^1.18.9",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-graphql-request": "^3.2.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-codegen/typescript-urql": "^2.0.10",
    "@graphql-codegen/urql-introspection": "1.3.1",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "prisma": "^2.26.0",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  }
}
